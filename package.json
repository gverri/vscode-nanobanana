{
  "name": "vscode-nanobanana",
  "displayName": "VS Code: Nano Banana",
  "description": "Generate graphs and diagrams with Gemini AI inside VS Code",
  "icon": "media/icon.png",
  "version": "0.0.2",
  "publisher": "guilherme-verri",
  "repository": {
    "type": "git",
    "url": "https://github.com/gverri/vscode-nanobanana"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "diagram",
    "graph",
    "flowchart",
    "gemini",
    "ai",
    "visualization"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "nanobanana",
          "title": "Nano Banana",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "nanobanana": [
        {
          "type": "webview",
          "id": "nanobanana.view",
          "name": "Nano Banana"
        }
      ]
    },
    "commands": [
      {
        "command": "nanobanana.open",
        "title": "Open Nano Banana",
        "category": "Nano Banana"
      },
      {
        "command": "nanobanana.generateWithSelection",
        "title": "Generate Diagram from Selection",
        "category": "Nano Banana"
      },
      {
        "command": "nanobanana.setApiKey",
        "title": "Set Gemini API Key",
        "category": "Nano Banana"
      }
    ],
    "configuration": {
      "title": "Nano Banana",
      "properties": {
        "nanobanana.defaultAspectRatio": {
          "type": "string",
          "default": "16:9",
          "enum": [
            "1:1",
            "3:4",
            "4:3",
            "9:16",
            "16:9"
          ],
          "description": "Default aspect ratio for generated diagrams"
        },
        "nanobanana.defaultModel": {
          "type": "string",
          "default": "gemini-3-pro-image-preview",
          "enum": [
            "gemini-3-pro-image-preview"
          ],
          "description": "Default Gemini model for diagram generation"
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "command": "nanobanana.generateWithSelection",
          "when": "editorHasSelection",
          "group": "nanobanana"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@google/genai": "^0.10.0"
  }
}
